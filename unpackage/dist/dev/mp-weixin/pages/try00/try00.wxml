<view class="container"><form><view class="form-item"><view class="label">预约时间：</view><view class="value">{{date}}</view></view><view class="form-item"><view class="label">预约用途：</view><input type="text" placeholder="请输入预约用途" data-event-opts="{{[['input',[['__set_model',['','purpose','$event',[]]]]]]}}" value="{{purpose}}" bindinput="__e"/></view><view class="form-item"><view class="label">预约人数：</view><input type="number" placeholder="请输入预约人数" data-event-opts="{{[['input',[['__set_model',['','num','$event',[]]]]]]}}" value="{{num}}" bindinput="__e"/></view><view class="form-item"><view class="label">联系人：</view><input type="text" placeholder="请输入联系人姓名" data-event-opts="{{[['input',[['__set_model',['','contactName','$event',[]]]]]]}}" value="{{contactName}}" bindinput="__e"/></view><view class="form-item"><view class="label">联系电话：</view><input type="tel" placeholder="请输入联系人电话" data-event-opts="{{[['input',[['__set_model',['','contactPhone','$event',[]]]]]]}}" value="{{contactPhone}}" bindinput="__e"/></view><button class="btn-submit" type="primary" data-event-opts="{{[['tap',[['submitForm',['$event']]]]]}}" bindtap="__e">提交预约</button></form></view>